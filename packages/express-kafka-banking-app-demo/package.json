{
  "name": "Kafka-Express-App",
  "version": "1.0.0",
  "description": "Demo app for previewing app for hypertest node sdk",
  "scripts": {
    "index": "HT_SDK_SERVER_PORT=3333 HT_SDK_SERVER_HOST=localhost HT_CLI_SERVER_HOST=localhost HT_CLI_SERVER_PORT=4444 HT_MODE=RECORD DEBUG=ht* node index.js",
    "start": "npm run dbSeed && kill-port 12301 && HT_MODE=RECORD DEBUG=ht* node index.js",
    "dbSeed": "docker compose up -d && node dbSeed.js",
    "simulate-traffic": "node simulateTraffic.js",
    "run-test": "kill-port 12301 && htcli start-new-test --config-file-path ./htTestConf.js",
    "run-test-cov": "nyc npm run run-test",
    "htcli-v": "htcli -v"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@hypertestco/node-sdk": "0.2.5-alpha.11",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.45.1",
    "@opentelemetry/resources": "^1.18.1",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@opentelemetry/semantic-conventions": "^1.18.1",
    "axios": "^1.6.2",
    "pg": "^8.11.3",
    "kafkajs":"^2.2.4",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@hypertestco/ht-cli": "0.2.5-alpha.11",
    "kill-port": "^2.0.1",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0"
  },
  "main": "index.js"
}
